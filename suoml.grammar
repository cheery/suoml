file:
    (=stmt)*

stmt:
    symbol >"(" sep(symbol, ",") ")" >expr / def

expr:
    symbol "=" expr =expr               / let
    "fn" "(" sep(symbol, ",") ")" expr  / abs
    "if" expr "then" expr "else" expr   / if_then_else
    term

term:
    "(" expr ")"
    term >"(" sep(expr, ",") ")" / app
    symbol / var
    string / string
    int    / int

terminal symbol, string, int
